{
  "name": "webpack-template-project",
  "version": "1.0.0",
  "description": "Version Inicial de proyecto configurado con webpack",
  "main": "./js/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "dev2": "webpack --mode development ./prueba/dev/main.js --output ./prueba/public/script.js",
    "build2": "webpack --mode production ./prueba/dev/main.js --output ./prueba/public/script.js",
    "dev-babel": "webpack --mode development --module-bind js=babel-loader",
    "build-babel": "webpack --mode production --module-bind js=babel-loader"
  },
  "author": "Wilson Benavides <benavidesb@unicauca.edu.co>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.13.1",
    "postcss-loader": "^2.1.6",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
